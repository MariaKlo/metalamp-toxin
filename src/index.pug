doctype html 
html
  head
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1, shrink-to-fit=no")
    meta(name="description", content="Preview")
    meta(name="keywords", content="metalamp, toxin, ui-kit")
    title Preview page
  body
    - var pages = {'Colors-Types': 'colors-type.html', 'Form-elements': 'form-elements.html', 'Cards': 'cards.html', 'Headers-Footers': 'headers-footers.html', 'Filter': 'filter.html', 'Landing-page': 'landing-page.html', 'Registration': 'registration.html', 'Room-details': 'room-details.html', 'Sign-in': 'sign-in.html'}
    div.preview
      each val, key in pages 
        a(href=val class="preview__link")= key